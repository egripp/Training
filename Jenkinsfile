		echo 'Build'
		    
		sh 'sleep 5'

            }
	    
        }
	    stage('Test') {
            steps {


		echo 'Step 2'
		    
		    echo 'Test'
		
		sh 'sleep 5'
		

            }
	    
        }
	    stage('Deploy') {
            steps {

		
		echo 'Step 3'
		    
		echo 'Deploy'
		    
		sh 'sleep 5'
		    
		
            }
	    
        }
    }
}

